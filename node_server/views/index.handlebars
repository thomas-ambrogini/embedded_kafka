<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="UTF-8">
    <title>{{title}}</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <script src="/javascripts/jquery.js"></script>
    <script src="/javascripts/dynamicLoading.js"></script>
  </head>
  <body>
    <div class="sidebar">
        <ul class="menu">
            {{#each menuItems}}
                <li><a href="#" class="load-page" data-url="{{url}}">{{label}}</a></li>
            {{/each}}
        </ul>
    </div>

    <div class="content">
        <h1>{{title}}</h1>
        <p>Welcome to {{title}}</p>
    </div>
  </body>
</html>